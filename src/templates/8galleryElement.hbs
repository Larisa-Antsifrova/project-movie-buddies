{{!-- {{#each this}} --}}
<li class="gallery-element">
    <img class="movie-image"
        src="{{ image }}" alt="{{ name }}">
    <h3 class="movie-name" >{{ name }}</h3>
    <p class="movie-genre">{{ genre }}</p>
    <p class="movie-year">{{ year }}</p>
    <p class="movie-rating">{{ rating }}</p>
</li>
{{!-- {{/each}} --}}